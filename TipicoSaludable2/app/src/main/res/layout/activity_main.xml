<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.sanchez.tipicosaludable.MainActivity"
    android:background="@drawable/fondoapp">


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="18dp">
        
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="5dp">
            <ImageView
                android:id="@+id/perfil"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:src="@drawable/perfil"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/perfil"
                android:layout_gravity="center"
                android:textSize="23sp"
                android:textColor="@color/blanco"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            >
            <ImageView
                android:id="@+id/inicio"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:src="@drawable/inicio"
                android:layout_margin="5dp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/inicio"
                android:layout_gravity="center"
                android:textSize="23sp"
                android:textColor="@color/blanco"
                />

        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="8dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="5dp">
            <ImageView
                android:id="@+id/comidas"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:src="@drawable/comidass"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/comidas"
                android:layout_gravity="center"
                android:textSize="23sp"
                android:textColor="@color/blanco"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="5dp">
            <ImageView
                android:id="@+id/ejercicios"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:src="@drawable/ejercicioa"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/ejercicios"
                android:layout_gravity="center"
                android:textSize="23sp"
                android:textColor="@color/blanco"
                />

        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="8dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="5dp">
            <ImageView
                android:id="@+id/actividadfisica"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:src="@drawable/actividadfisica"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/Afisica"
                android:layout_gravity="center"
                android:textSize="23sp"
                android:textColor="@color/blanco"
                />

        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_margin="5dp">
            <ImageView
                android:id="@+id/deportes"
                android:layout_width="110dp"
                android:layout_height="110dp"
                android:src="@drawable/deporte"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/deportes"
                android:layout_gravity="center"
                android:textSize="23sp"
                android:textColor="@color/blanco"
                />

        </LinearLayout>
    </LinearLayout>






 </LinearLayout>

